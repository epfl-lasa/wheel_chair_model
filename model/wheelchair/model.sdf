<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="wheelchair">
    <!-- <pose>0 0 10 0 0 0</pose> -->
    <pose>0 0 0.1 0 0 0</pose>
    <static>false</static>
    <link name="Mainbody">
      <pose>0 0 0 1.570796 0 0</pose>
      <inertial>
        <mass>1</mass>
        <inertia> <!-- inertias are tricky to compute -->
          <!-- http://gazebosim.org/tutorials?tut=inertia&cat=build_robot -->
          <ixx>0.0</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
          <ixy>0.0</ixy>         <!-- for a box: ixy = 0 -->
          <ixz>0.0</ixz>         <!-- for a box: ixz = 0 -->
          <iyy>0.0</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <iyz>0.0</iyz>         <!-- for a box: iyz = 0 -->
          <izz>0.0</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
         <!--  <ixx>12.10</ixx> -->       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
         <!--  <ixy>0.03</ixy> -->         <!-- for a box: ixy = 0 -->
          <!-- <ixz>2.70</ixz>     -->     <!-- for a box: ixz = 0 -->
          <!-- <iyy>13.0</iyy>      -->  <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <!-- <iyz>0.10</iyz>       -->   <!-- for a box: iyz = 0 -->
         <!--  <izz>9.0</izz>      -->  <!-- for a box: izz = 0.083 * mass * (x*x + y*y) --> 
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>model://wheelchair/meshes/Qolo_T_CB_base.STL</uri>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>model://wheelchair/meshes/Qolo_T_CB_base.STL</uri>
          </mesh>
        </geometry>
      </visual>
	  
      <!-- Add a ray sensor, and give it a name -->
      <sensor type="ray" name="lidar_front">
        <pose>0.4 0 0.2 0 0 0</pose>
        <visualize>true</visualize>
        <update_rate>30</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>32</samples>
              <resolution>1</resolution>
              <min_angle>-0.53529248</min_angle>
              <max_angle>0.18622663</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.05</min>
            <max>70</max>
            <resolution>0.02</resolution>
          </range>
        </ray>        
      </sensor>
      <sensor type="ray" name="lidar_back">
        <!-- <pose>-0.4 0 0.2 1.5707 0 3.1415</pose> -->
        <pose>-0.4 0 0.2 0 0 3.1415</pose>
        <visualize>true</visualize>
        <update_rate>30</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>32</samples>
              <resolution>1</resolution>
              <min_angle>-0.53529248</min_angle>
              <max_angle>0.18622663</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.05</min>
            <max>70</max>
            <resolution>0.02</resolution>
          </range>
        </ray>        
      </sensor>
    </link>
	
    <link name="left_main_wheel">
      <pose>0.0865 -0.030 0.199 1.570796 0 0</pose>
	  <!-- <pose>-0.091 -0.33 0.109 0 1.57 1.57</pose> -->
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>.199</radius>
            <length>.04</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1000000.0</mu>
              <mu2>1000000.0</mu2>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
		  <cylinder>
            <radius>.199</radius>
            <length>.04</length>
          </cylinder>
          <!-- </<mesh> -->
            <!-- <scale>0.001 0.001 0.001</scale> -->
            <!-- <uri>model://meshes/wheelJoyUnitPlus.STL</uri> -->
		  <!-- </mesh> -->
        </geometry>
      </visual>
    </link>
    <joint type="revolute" name="left_wheel_main_hinge">
      <pose>0 0 -0.0 0 0 0</pose>
      <child>left_main_wheel</child>
      <parent>Mainbody</parent>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
      <sensor name="force_torque_left_wheel_main_hinge" type="force_torque">
        <update_rate>100</update_rate>
        <visualize>true</visualize>
      </sensor>
    </joint>
	
